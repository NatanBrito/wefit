{
    "name": "teste-backend",
    "version": "1.0.0",
    "main": "index.js",
    "type": "module",
    "license": "MIT",
    "scripts": {
        "start": "ts-node-dev --files -r tsconfig-paths/register --inspect --ignore-watch node_modules src/server.ts",
        "teste_script": "npm i && npm run prisma:deploy && npm run start",
        "prisma:migrate": "npx prisma migrate dev --name init",
        "prisma:deploy": "npx prisma migrate deploy"
    },
    "dependencies": {
        "@prisma/client": "^5.21.0",
        "cors": "^2.8.5",
        "express": "^4.17.3"
    },
    "devDependencies": {
        "@types/cors": "^2.8.17",
        "@types/express": "^4.17.13",
        "@types/node": "^22.7.5",
        "prisma": "^5.21.0",
        "ts-node-dev": "^1.1.8",
        "tsconfig-paths": "^3.12.0",
        "typescript": "^4.6.2"
    }
}
